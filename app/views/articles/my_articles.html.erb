<div class="grid grid-cols">
  <h1>List of My Articles</h1>
  <div class="grid grid-cols ml-8 mt-4">
    <% if user_signed_in? %>
      <% @my_articles.each do |article| %>
        <li><%= link_to article.title, article %></li>
      <% end %>
    <% end %>
  </div>
  <div class="mt-4">
    <%=link_to "Back", articles_path, class:"border border-gray-300 p-1 rounded mt-4" %>
  </div>

</div>